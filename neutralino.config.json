{
  "applicationId": "dev.neukit.bun.starter",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/dist/",
  "port": 0,
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "exportAuthInfo": false,
  "distributionPath": "/release",
  "enableExtensions": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "custom.*",
    "events.*",
    "extensions.*",
    "os.*",
    "debug.log",
    "filesystem.*",
    "computer.*"
  ],
  "modes": {
    "window": {
      "title": "NeuKitStarter",
      "icon": "/assets/icon.png",
      "width": 800,
      "height": 800,
      "minWidth": 400,
      "minHeight": 200,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true,
      "injectGlobals": true
    }
  },
  "buildScript": {
    "mac": {
      "architecture": [
        "x64",
        "arm64"
      ],
      "minimumOS": "10.13.0",
      "appName": "NeuKitStarter",
      "appBundleName": "NeuKitStarter"
    },
    "win": {
      "architecture": [
        "x64"
      ],
      "appName": "NeuKitStarter",
      "appIcon": "./assets/icon.ico"
    },
    "linux": {
      "architecture": [
        "x64",
        "arm64",
        "armhf"
      ],
      "appName": "NeuKitStarter",
      "appIcon": "./assets/icon.png",
      "appPath": "/usr/share/NeuKitStarter",
      "appIconPath": "/usr/share/NeuKitStarter/icon.png"
    }
  },
  "cli": {
    "binaryName": "neu-starter",
    "resourcesPath": "/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.1.0",
    "clientVersion": "6.1.0",
    "frontendLibrary": {
      "patchFile": "/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/src",
      "initCommand": "bun install",
      "devCommand": "bun run dev:vite",
      "buildCommand": "bun run build",
      "waitTimeout": 20000
    }
  },
  "extensions": [
    {
      "id": "dev.neukit.bun-server.ext",
      "commandLinux": "${NL_PATH}/extensions/bun-server/start",
      "commandDarwin": "${NL_PATH}/extensions/bun-server/start",
      "commandWindows": "${NL_PATH}/extensions/bun-server/start.exe"
    }
  ]
}
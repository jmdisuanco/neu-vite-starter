{
  "name": "neukit-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev": "bun run neukit:bun-server && neu run  -- --dev --window-enable-inspector",
    "neukit:build-bun-server": "bun build ./extensions-src/bun-server/index.ts --compile  --target=bun-darwin-arm64 --bytecode  --minify --outfile  ./extensions/bun-server/start",
    "neukit:build-bun-server:mac": "bun build ./extensions-src/bun-server/index.ts --compile --target=bun-darwin-arm64 --bytecode  --minify --outfile ./extensions/bun-server/start-mac_arm64 && bun build ./extensions-src/bun-server/index.ts --compile --targe=bun-darwn-x64 --bytecode  --minify --outfile ./extensions/bun-server/start-mac_x64",
    "neukit:build-bun-server:linux": "bun build ./extensions-src/bun-server/index.ts --compile --target=bun-linux-arm64 --bytecode  --minify --outfile ./extensions/bun-server/start-linux_arm64 && bun build ./extensions-src/bun-server/index.ts --compile --targe=bun-linux-x64 --bytecode  --minify --outfile ./extensions/bun-server/start-linux_x64 &&  bun build ./index.ts --compile --targe=bun-linux-armhf --bytecode  --minify --outfile ./extensions/bun-server/start-linux_armhf",
    "neukit:build-bun-server:windows": " bun build ./extensions-src/bun-server/index.ts --compile --target=bun-windows-x64 --bytecode  --minify --outfile  ./extensions/bun-server/start-windows_x64.exe",
    "neukit:build-bun-server:all": "bun run build:mac && bun run build:linux && bun run build:windows",
    "neukit:prepackage:mac": "bun run neukit:build-bun-server:mac",
    "neukit:prepackage:linux": "bun run neukit:build-bun-server:linux",
    "neukit:prepackage:windows": "bun run neukit:build-bun-server:windows",
    "tag:patch": "bun scripts/tag.js patch",
    "tag:minor": "bun scripts/tag.js minor",
    "tag:major": "bun scripts/tag.js major"
  },
  "dependencies": {
    "@neutralinojs/lib": "^6.1.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "tw-animate-css": "^1.3.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
